import java.sql.*;

public class Conn {

	public static void main(String[] args) throws SQLException {
		// TODO Auto-generated method stub
//		Driver Register
		String url = "jdbc:mysql://localhost:3306";
		String user= "root";
		String pass = "";
		DriverManager.getConnection(url,user,pass);
		System.out.println("Connected");

		
	}

}
